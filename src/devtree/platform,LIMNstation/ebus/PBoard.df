#include "<df>/dragonfruit.h"
#include "<inc>/a3x.h"

extern BuildCitron

procedure BuildPBoard (* -- *)
	DeviceNew
		"platformboard" DSetName

		"AISA,platformboard" "model" DAddProperty

		PBInfo@ "version" DAddProperty

		0xF8000000 "platformboard,base" DAddProperty

		BuildCitron
	DeviceExit
end